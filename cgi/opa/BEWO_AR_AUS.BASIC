002330                CALL CGI.TEMPLATE.VAR("para10",PARA10$)
002340                RC$ = FN.WFFOOTER$(NIX$) ! Fußzeile
002350                CALL CGI.TEMPLATE("/wwwroot/opa/tpl/bewo_ar_aus.tpl")
002360                GOTO ENDE
002370           OTHERWISE 
