002410 ! ********************* HAUPTPROGRAMM ENDE ***********************
002420 FEHLER:
002430      CALL CGI.TEMPLATE.VAR("msg1",MSG$(1))
002440      CALL CGI.TEMPLATE.VAR("msg2",MSG$(2))
