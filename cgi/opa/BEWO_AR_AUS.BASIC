002530 REM *******************************************************
002540 KOPF: ! Kopfzeile für PDF ausgabe
002550      SZ% = SZ%+1 ! Seitenzähler
002560      PRINT #23:"B E W O H N E R A R T I K E L"
002570      PRINT #23:"Bewohnername: "&TRIM$(TR63$);SPACE$(4);"Datum: "&DATE$(0);SPACE$(4);"Uhrzeit: "&TIME$(0);SPACE$(10)&"Seite: "&STR$(SZ%)
002580      PRINT #23
002590      PP% = PP%+1 ! Zeilenzähler
002600      IF BARV%=1 THEN PRINT #23:"Pos";TAB(5);"Art-Nr.";TAB(13);"Artikelbezeichnung";TAB(40);"Barcode";TAB(54);"Angelegt";TAB(64);"Eingang";TAB(74);"Ausgang";TAB(83);"Seq";TAB(88);"Status" ELSE PRINT #23:"Pos";TAB(5);"Art-Nr.";TAB(13);"Artikelbezeichnung";TAB(40);"Barcode";TAB(54);"Angelegt";TAB(83);"Seq" ! PDF Kopf anpassen für vollständige/unvollständige Liste
002610      PRINT #23:RPT$(132,"-")
002620      RETURN 
002630 REM *******************************************************
002640 PDF: ! Dateiausgabe PDF
002650      PP% = PP%+1 ! Zeilenzähler
002660      PRINT #23:PDFZL$
002670      IF PP%=38 ! Seitenwechsel/Seitenumbruch
002680           PRINT #23:SPACE$(50);"*** Ü B E R T R A G ***"
002690           CLEAR PP%
002700           GOSUB KOPF
002710           IFEND 
002720      IF SORTLAUF%=1000 ! letzter Lauf -ergo- Fußzeile
002730           PRINT #23:SPACE$(132)
002740           PRINT #23:LEGENDE.PDF$
002750           PRINT #23
002760           IFEND 
002770      RETURN 
002780 REM *******************************************************
002790 L21: ! Barcode Bewohnerartikel BEWO21.REI
002800      FL% = 21 \ READ #21,KY21$:AKY21$,TRKD,TRAR,TRBEWO,TR61$,TR62$,TR63$,TR64$,TR65$,TR66,TR67,TR68$,TR71,TR72$,TR73$,TR74%,TR75%,TR76%,TR77,TR41,TR42,TR43,TR44,TR45,TR51$,TR52,TR53,TR54$,TR55$,TR90,TR91,TR92,TR93,TR94%,TR95%,TR96%,TR97%,XX1$,XX2$ \ UNLOCK #21 \ CLEAR FL%
002810      RETURN 
002820 L22: ! Index Bewohnerartikel BEWO22.REI
